    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-image": "^3.0.0",
    "nodemailer": "^6.4.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.29",
